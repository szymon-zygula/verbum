{
  "language": {
    "symbols": [
      "+",
      "-",
      "*",
      "/",
      "sin",
      "cos",
      "<<",
      ">>"
    ]
  },
  "rules": [
    {
      "from": "(* $0 2)",
      "to": "(<< $0 1)"
    },
    {
      "from": "(* $0 1)",
      "to": "$0"
    },
    {
      "from": "(/ (* $0 $1) $2)",
      "to": "(* $0 (/ $1 $2))"
    },
    {
      "from": "(/ $0 $0)",
      "to": "1"
    }
  ]
}