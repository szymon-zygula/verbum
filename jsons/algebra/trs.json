{
  "rules": [
    {
      "from": "(+ $0 $1)",
      "to": "(+ $1 $0)"
    },
    {
      "from": "(* $0 $1)",
      "to": "(* $1 $0)"
    },
    {
      "from": "(+ (+ $0 $1) $2)",
      "to": "(+ $0 (+ $1 $2))"
    },
    {
      "from": "(* (* $0 $1) $2)",
      "to": "(* $0 (* $1 $2))"
    },
    {
      "from": "(+ $0 0)",
      "to": "$0"
    },
    {
      "from": "(* $0 1)",
      "to": "$0"
    },
    {
      "from": "(* $0 0)",
      "to": "0"
    },
    {
      "from": "(neg (neg $0))",
      "to": "$0"
    },
    {
      "from": "(+ $0 (neg $0))",
      "to": "0"
    },
    {
      "from": "(- $0 $1)",
      "to": "(+ $0 (neg $1))"
    },
    {
      "from": "(neg (+ $0 $1))",
      "to": "(+ (neg $0) (neg $1))"
    },
    {
      "from": "(neg (* $0 $1))",
      "to": "(* (neg $0) $1)"
    },
    {
      "from": "(* $0 (+ $1 $2))",
      "to": "(+ (* $0 $1) (* $0 $2))"
    },
    {
      "from": "(+ (* $0 $1) (* $0 $2))",
      "to": "(* $0 (+ $1 $2))"
    },
    {
      "from": "(pow $0 1)",
      "to": "$0"
    },
    {
      "from": "(pow $0 0)",
      "to": "1"
    },
    {
      "from": "(pow 1 $0)",
      "to": "1"
    },
    {
      "from": "(pow 0 $0)",
      "to": "0"
    },
    {
      "from": "(pow $0 2)",
      "to": "(* $0 $0)"
    },
    {
      "from": "(* $0 (pow $0 2))",
      "to": "(pow $0 3)"
    },
    {
      "from": "(pow $0 3)",
      "to": "(* $0 (pow $0 2))"
    },
    {
      "from": "(+ $0 $0)",
      "to": "(* 2 $0)"
    },
    {
      "from": "(* $0 2)",
      "to": "(+ $0 $0)"
    },
    {
      "from": "(* $0 (neg 1))",
      "to": "(neg $0)"
    },
    {
      "from": "(neg 0)",
      "to": "0"
    },
    {
      "from": "(pow (pow $0 $1) $2)",
      "to": "(pow $0 (* $1 $2))"
    },
    {
      "from": "(* (pow $0 $1) (pow $0 $2))",
      "to": "(pow $0 (+ $1 $2))"
    },
    {
      "from": "(pow $0 (+ $1 $2))",
      "to": "(* (pow $0 $1) (pow $0 $2))"
    }
  ]
}
