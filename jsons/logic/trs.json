{
  "rules": [
    {
      "from": "(and $0 $1)",
      "to": "(and $1 $0)"
    },
    {
      "from": "(and $0 1)",
      "to": "$0"
    },
    {
      "from": "(and $0 0)",
      "to": "0"
    },
    {
      "from": "(or $0 $1)",
      "to": "(or $1 $0)"
    },
    {
      "from": "(or $0 0)",
      "to": "$0"
    },
    {
      "from": "(or $0 1)",
      "to": "1"
    },
    {
      "from": "(not (not $0))",
      "to": "$0"
    },
    {
      "from": "(not 1)",
      "to": "0"
    },
    {
      "from": "(not 0)",
      "to": "1"
    },
    {
      "from": "(xor $0 $1)",
      "to": "(xor $1 $0)"
    },
    {
      "from": "(xor $0 0)",
      "to": "$0"
    },
    {
      "from": "(xor $0 1)",
      "to": "(not $0)"
    },
    {
      "from": "(xor $0 $0)",
      "to": "0"
    },
    {
      "from": "(imp $0 $1)",
      "to": "(or (not $0) $1)"
    },
    {
      "from": "(iff $0 $1)",
      "to": "(and (imp $0 $1) (imp $1 $0))"
    }
  ]
}
