{
  "rules": [
    {
      "from": "(+ 0 $0)",
      "to": "$0"
    },
    {
      "from": "(+ $0 0)",
      "to": "$0"
    },
    {
      "from": "(* 1 $0)",
      "to": "$0"
    },
    {
      "from": "(* $0 1)",
      "to": "$0"
    },
    {
      "from": "(+ $0 $1)",
      "to": "(+ $1 $0)"
    },
    {
      "from": "(* $0 $1)",
      "to": "(* $1 $0)"
    },
    {
      "from": "(* $0 2)",
      "to": "(<< $0 1)"
    }
  ]
}
